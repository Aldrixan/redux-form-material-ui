{
    "name": "redux-form-material-ui-adapter",
    "version": "6.1.1",
    "description": "An adapter between Redux Form and Material UI components",
    "main": "./lib/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Aldrixan/redux-form-material-ui"
    },
    "scripts": {
        "build": "npx babel src --out-dir lib --ignore '**/*.spec.js'",
        "clean": "rimraf dist lib",
        "lint": "eslint src",
        "prepublish": "npm run test && npm run lint && npm run clean && npm run build",
        "test": "jest",
        "test:codecov": "jest --coverage --coverageReporters=text-lcov | coveralls",
        "test:watch": "npm test -- --watch"
    },
    "keywords": [
        "redux",
        "redux-form",
        "material-ui",
        "redux-form-material-ui",
        "adapter"
    ],
    "author": "Matthew Watts <wattsm2008@gmail.com> (http://github.com/Aldrixan)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Aldrixan/redux-form-material-ui/issues"
    },
    "homepage": "https://github.com/Aldrixan/redux-form-material-ui",
    "devDependencies": {
        "@babel/cli": "^7.6.2",
        "@babel/core": "^7.6.2",
        "@babel/preset-env": "^7.6.2",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.6.2",
        "@material-ui/core": "^4.5.0",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "coveralls": "^3.0.6",
        "cross-env": "^6.0.3",
        "eslint": "^6.5.1",
        "eslint-config-react-app": "^5.0.2",
        "eslint-plugin-flowtype": "^4.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.16.0",
        "eslint-plugin-react-hooks": "^2.1.2",
        "expect": "^24.9.0",
        "expect-jsx": "^5.0.0",
        "isparta": "^4.1.1",
        "jest": "^24.9.0",
        "jsdom": "^15.1.1",
        "lodash.noop": "^3.0.1",
        "mocha": "^6.2.1",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^14.1.1",
        "prettier": "^1.18.2",
        "react": "^16.10.2",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.10.2",
        "react-tap-event-plugin": "^3.0.3",
        "react-test-renderer": "^16.10.2",
        "rimraf": "^3.0.0",
        "webpack": "^4.41.0"
    },
    "peerDependencies": {
        "@material-ui/core": "^3.0.0",
        "react": "^15.0.0 || ^16.0.0 ",
        "redux-form": "^6.0.0 || ^7.0.0"
    },
    "files": [
        "README.md",
        "lib",
        "index.d.ts"
    ],
    "nyc": {
        "include": [
            "src/**/*.js"
        ],
        "sourceMap": false,
        "instrument": false
    },
    "npmName": "redux-form-material-ui-adapter",
    "npmFileMap": [
        {
            "basePath": "/dist/",
            "files": [
                "*.js"
            ]
        }
    ],
    "dependencies": {
        "@babel/polyfill": "^7.6.0",
        "@material-ui/pickers": "^3.2.7",
        "@types/material-ui": "^0.21.7",
        "core-js": "^3.2.1"
    }
}
